#ifdef __cplusplus
extern "C" {
#endif
#include "xio_switch.h"
#ifdef __cplusplus
}
#endif

#include "gpio.h"
#include <xparameters.h>

int main()
{
	gpio g1;
	XGpio xgpio;
	set_pin(0, GPIO);
	XGpio_Initialize(&xgpio, XPAR_IO_SWITCH_0_GPIO_BASEADDR);
	g1 = gpio_open(0);
	gpio_set_direction(g1, GPIO_OUT);
	while (1) {
		gpio_write(g1, 0);
		gpio_write(g1, 1);
	}
	return 0;
}
